{
  "name": "orchestrator-horarioappufps",
  "version": "2.0.0",
  "description": "Orchestrator for Web Scrappers in HorarioAppUFPS",
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter-ts.git"
  },
  "keywords": [],
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "swagger": "tsoa spec && tsoa routes",
    "build": "pnpm run clean && pnpm run swagger && tsc",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx src/db/seed.ts",
    "start:prod": "prisma migrate deploy && node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@tsoa/runtime": "^6.6.0",
    "bullmq": "^5.61.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsdom": "^26.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.18.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.30.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsoa": "^6.6.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}